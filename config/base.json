{
  "bots": [
    {
      "name": "GNU Go 3.8",
      "path": "./engine/gnugo",
      "command": "gnugo",
      "parameters": "--mode gtp --japanese-rules",
      "main": 1
    },
    {
      "name": "Pachi 12.60",
      "path": "./engine/Pachi-12.60",
      "command": "pachi.exe",
      "parameters": "-r japanese --smart-pass",
      "enabled": 0
    },
    {
      "name": "Katago #kata1 b28",
      "path": "./engine/katago",
      "command": "katago",
      "parameters": "gtp -model ./models/kata1-b28c512nbt-s9584861952-d4960414494.bin.gz -config ./default_gtp.cfg",
      "enabled": 0,
      "kibitz": 0,
      "messages": [
          {
            "regex": "^:\\s+T.*--\\s*([A-Z0-9]+)",
            "output": "$1",
            "var": "$primaryMove"
          },
          {
            "regex": "^$primaryMove.*(W\\s+[^\\s]+).*\\(\\s*([^\\s]+\\s+L)",
            "output": "$1 $2"
          },
          {
            "regex": "Controller:",
            "output": " "
          }
      ]
    },
    {
      "name": "Katago #kata9x9 b18",
      "path": "./engine/katago",
      "command": "katago",
      "parameters": "gtp -model ./models/kata9x9-b18c384nbt-20231025.bin.gz -config ./default_gtp.cfg",
      "enabled": 0,
      "kibitz": 0,
      "messages": [
          {
            "regex": "^:\\s+T.*--\\s*([A-Z0-9]+)",
            "output": "$1",
            "var": "$primaryMove"
          },
          {
            "regex": "^$primaryMove.*(W\\s+[^\\s]+).*\\(\\s*([^\\s]+\\s+L)",
            "output": "$1 $2"
          },
          {
            "regex": "Controller:",
            "output": " "
          }
      ]
    },
    {
      "name": "Zen 6",
      "path": "./engine/zen",
      "command": "wine",
      "parameters": "./engine/zen/gtp4zen.exe -z 6 -t 2 -T 120 -s 2147483647 -r 25",
      "enabled": 0,
      "kibitz": 0,
      "main": 0
    },
    {
      "name": "Zen 7",
      "path": "./engine/zen",
      "command": "wine",
      "parameters": "./engine/zen/gtp4zen.exe -z 7 -t 2 -T 120 -s 2147483647 -r 25",
      "enabled": 0,
      "kibitz": 0,
      "main": 0
    }
  ],
  "controls": [
    {"key": 1, "command": "play once"},
    {"key": 81, "command": "quit"},
    {"key": 17, "command": "toggle_fullscreen"},
    {"key": 35, "command": "fps"},
    {"key": 26, "command": "animate"},
    {"key": 31, "command": "toggle_territory"},
    {"key": 25, "command": "toggle_overlay"},
    {"key": 29, "command": "resign"},
    {"key": 27, "command": "pass"},
    {"key": 14, "command": "reset camera"},
    {"key": 32, "command": "undo move"},
    {"key": 15, "command": "pan camera"},
    {"key": 12, "command": "rotate camera"},
    {"key": 30, "command": "zoom camera"},
    {"key": 33, "command": "cycle shaders"},
    {"key": 47, "command": "increase gamma"},
    {"key": 45, "command": "decrease gamma"},
    {"key": 39, "command": "increase contrast"},
    {"key": 41, "command": "decrease contrast"},
    {"key": 46, "command": "reset contrast and gamma"},
    {"key": 19, "command": "increase eof"},
    {"key": 23, "command": "decrease eof"},
    {"key": 21, "command": "increase dof"},
    {"key": 22, "command": "decrease dof"},
    {"key": 72, "command": "toggle_analysis_mode"},
    {"key": 89, "command": "navigate_start"},
    {"key": 88, "command": "navigate_end"}
  ],
  "sounds": {
    "move": "./config/sound/stone.wav",
    "clash": "./config/sound/collision.wav"
  },
  "fonts": {
    "gui": [
      "./config/fonts/NotoSans-Regular.ttf",
      "./config/fonts/NotoSansCJKsc-Regular.otf"
    ],
    "overlay": "./config/fonts/default-font.ttf"
  },
  "shaders": [
    {
      "name": "Red Carpet",
      "vertex": "./config/shaders/vertex/mono.glsl",
      "fragment": "./config/shaders/fragment/red_carpet.glsl",
      "height": 0.85
    },
    {
      "name": "Minimal Thin",
      "vertex": "./config/shaders/vertex/mono.glsl",
      "fragment": "./config/shaders/fragment/thin.glsl",
      "height": 0.85
    },
    {
      "name": "Minimal Flat",
      "vertex": "./config/shaders/vertex/mono.glsl",
      "fragment": "./config/shaders/fragment/flat.glsl",
      "height": 0.85
    },
    {
      "name": "Minimal 2D",
      "vertex": "./config/shaders/vertex/mono.glsl",
      "fragment": "./config/shaders/fragment/2d.glsl",
      "height": 0.0
    },
    {
      "name": "[stereo] Red Carpet",
      "vertex": "./config/shaders/vertex/stereo.glsl",
      "fragment": "./config/shaders/fragment/red_carpet_stereo.glsl",
      "height": 0.85
    },
    {
      "name": "[stereo] Minimal Thin",
      "vertex": "./config/shaders/vertex/stereo.glsl",
      "fragment": "./config/shaders/fragment/thin_stereo.glsl",
      "height": 0.85
    }
  ],
  "overlay_shaders": {
    "vertex": "./config/shaders/overlay/overlay-vertex.glsl",
    "fragment": "./config/shaders/overlay/overlay-fragment.glsl",
    "atlas": "./config/shaders/overlay/overlay-atlas.glsl"
  },
  "sgf_dialog": {
    "game_columns": ["index", "board", "black", "white", "moves", "result"],
    "games_path": "./games"
  }
}
